<h1>Here are a list of your Workouts</h1>

<div>
  <%= form_for(@workout) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :date %>
      <%= f.date_select :date, max: Time.now %>
      <%=f.submit %>
  <% end %>
</div>

<ul>
  <%@workouts.each do |workout| %>
      <li><%= link_to workout.name, workout_path(workout)%></li>
  <% end %>

</ul>